{% extends 'back/base.html.twig' %}

{% block body %}
    <h1>Member creation</h1>
	{{ form_start(form) }}
		<div id="inputs">
			<div id="texts">
				{% for key, field in form %}
					{% if key != 'description' %}
						{{ form_widget(field) }}
					{% endif %}
				{% endfor %}
			</div>
			{{ form_widget(form.description) }}
		</div>
        <input type="submit" value="Envoyer" />
    {{ form_end(form) }}
{% endblock %}

